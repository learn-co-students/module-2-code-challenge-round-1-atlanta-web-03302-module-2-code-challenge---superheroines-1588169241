<ul>
<% if !@heroine.errors.empty? %>
    <% @heroine.errors.full_messages.each do |message|%>
        <li><%= message %></li>
    <% end %>
<% end %>
</ul>
<br>
<%= form_for(@heroine) do |f|%>
    <%= f.label :name%>
    <%= f.text_field :name%> <br>

    <%= f.label :super_name%>
    <%= f.text_field :super_name%> <br>

    <%= f.collection_select :power_ids, Power.all, :id, :name%> <br>

    <%= f.submit %>
<% end %>

<%= link_to "All Heroines", heroines_path%>